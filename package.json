{
  "name": "jira-slack-integration",
  "version": "1.0.0",
  "repository": "https://github.com/hjemmel/jira-slack-integration",
  "author": "Hans Jakob Emmel <hans.emmel@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "node dist/index.js",
    "build": "ts-node build.ts && tsc",
    "dev": "cross-env NODE_ENV=development nodemon server/index.ts | pino-pretty",
    "dev:debug": "cross-env NODE_ENV=development nodemon --inspect server/index.ts | pino-pretty",
    "test": "cross-env DOTENV_CONFIG_PATH=.env.test jest --colors",
    "test:coverage": "cross-env DOTENV_CONFIG_PATH=.env.test jest --colors --coverage",
    "test:ci": "cross-env DOTENV_CONFIG_PATH=.env.test jest --ci --runInBand --reporters=default --reporters=jest-junit",
    "lint": "eslint 'server/**/*.ts'",
    "type:check": "tsc --noEmit"
  },
  "dependencies": {
    "bluebird": "3.7.2",
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.4",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-openapi-validator": "3.10.0",
    "http-status-codes": "1.4.0",
    "pino": "5.17.0",
    "swagger-ui-express": "4.1.4"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.30",
    "@types/cookie-parser": "1.4.2",
    "@types/dotenv": "6.1.1",
    "@types/express-serve-static-core": "4.17.2",
    "@types/jest": "25.1.4",
    "@types/multer": "1.4.2",
    "@types/pino": "5.15.5",
    "@types/shelljs": "0.8.6",
    "@types/swagger-ui-express": "4.1.2",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "cross-env": "7.0.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-prettier": "3.1.2",
    "jest": "25.2.3",
    "jest-junit": "10.0.0",
    "nodemon": "2.0.2",
    "pino-pretty": "3.6.1",
    "prettier": "1.19.1",
    "shelljs": "0.8.3",
    "ts-jest": "25.2.1",
    "ts-node": "8.6.2",
    "typescript": "3.8.3"
  }
}
